{
  "name": "vercel-shortener-api",
  "version": "1.0.0",
  "description": "A simple and scalable URL shortener API built with Node.js, Express, and MongoDB, ready for Vercel deployment.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npm run build:css && node index.js",
    "build:css": "tailwindcss -i ./styles/input.css -o ./public/output.css",
    "dev": "concurrently \"nodemon index.js\" \"tailwindcss -i ./styles/input.css -o ./public/output.css --watch\""
  },
  "keywords": [
    "url-shortener",
    "express",
    "mongodb",
    "vercel",
    "api"
  ],
  "author": "Your Name (or leave blank)",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "mongoose": "^8.4.1",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.3",
    "tailwindcss": "^3.4.4"
  }
}
